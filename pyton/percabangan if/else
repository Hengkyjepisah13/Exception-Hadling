print("===== Program Grade Nilai=====")
nilai = int ("input nilai akhir:")
if nilai >= 90 :
    grade = "A"
elif nilai >= 80 :
    grade = "B"
elif nilai >= 70 :
    grade = "C"
elif nilai >= 60 :
    grade = "D"
elif nilai >= 50 :
    grade = "E"
elif nilai >= 40 :
    grade = "F"
print ("Grade Anda: ",grade)
